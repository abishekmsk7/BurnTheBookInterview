
<!doctype html>
<html  class="overflow-x-hidden no-js" lang="{{ craft.app.language|slice(0,2) }}">
   <head>
      <meta charset="utf-8">
      <meta http-equiv="x-ua-compatible" content="ie=edge">
      <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
      <link href='https://fonts.googleapis.com/css?family=Inter' rel='stylesheet'>
      <script src="https://cdn.tailwindcss.com"></script>
      <link href="https://fonts.googleapis.com/css2?family=Inter:wght@400;500;700&display=swap" rel="stylesheet">
      <link href="{{ siteUrl('css/styles.css')}}" rel="stylesheet">
      <script>
         tailwind.config = {
            theme: {
            extend: {
               colors: {
                  navy: '#1A3E65',
                  blue: '#008FF9',
                  black: '#000000',
                  white: '#ffffff',
                  lightblue: '#F3F4F6',
               },
         fontSize: theme => ({
            'xxs':   '0.7rem',  // 12
            'xs':   '0.75rem',  // 12
            'sm':   '0.875rem', // 14
            'base': '1rem',     // 16
            'md':   '1.125rem', // 18
            'lg':   '1.25rem',  // 20
            'xl':   '1.375rem',   // 22
            '2xl':  '2rem', // 30
            '3xl':  '2.5rem',  // 40
            '4xl':  '3rem',     // 48
            '5xl':  '3.625rem',  // 58
            '6xl':  '4.5rem',   // 72
            '7xl':  '6rem',     // 96
            '8xl':  '8rem',     // 128
            '9xl':  '10rem',    // 160
            }),
            }
            }
         }
      </script>
   </head>
   <body>
      <div id="app">
         {% block body %}{% endblock %}
      </div>
   </body>
</html>


